{
  "expo": {
    "name": "Nando",
    "slug": "nando",
    "version": "1.0.0",
    "platforms": ["ios", "android", "web"], 
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF4500"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nandoit.nando"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FF4500"
      },
      "package": "com.nandoit.nando"
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png",
      "bundler": "webpack",
      "build": {
        "mimeTypes": {
          ".wasm": "application/wasm"
        }
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://nando-app.netlify.app"
        }
      ]
    ]
  }
}